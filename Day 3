Que:-  https://leetcode.com/problems/palindrome-number/

class Solution {
public:
    bool isPalindrome(int x) {
        int a = x;
        long s=0;
        while (x>0)
        {
            int r=x%10;
            s=s*10+r;
            x=x/10;
        }
        if (a==s)
        return true;
        else 
        return false;
        
    }
};
